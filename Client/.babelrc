{
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "browsers": ["last 2 versions"]
          },
          "debug": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "universal-import"
    ],
    "env": {
      "development": {
        "plugins": ["react-hot-loader/babel"]
      }
    }
  }